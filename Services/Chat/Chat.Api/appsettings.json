{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Elasticsearch" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://es01:9200",
          "indexFormat": "my-logs-{0:yyyy.MM.dd}",
          "autoRegisterTemplate": true
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "ChatDbConnection": {
    "CHAT_DB_USER": "",
    "CHAT_DB_PASSWORD": "",
    "CHAT_DB_HOST": "",
    "CHAT_DB": "",
    "CHAT_DB_PORT": ""
  },
  "MessageBroker": {
    "Host" : "amqp://rabbit_mq:5672",
    "UserName" : "",
    "Password" : ""
  },
  "AllowedHosts": "*",
  "Test": "production",
  "OtlpExporter" : {
    "Uri" : ""
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1"
    }
  }
}

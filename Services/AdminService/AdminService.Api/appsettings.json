{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "ConnectionStrings": {
    "PostgresSQL": ""
  },
  "MessageBroker": {
    "Host" : "amqp://localhost:5672",
    "UserName" : "",
    "Password" : ""
  },
  "AllowedHosts": "*",
  "OtlpExporter" : {
    "Uri" : ""
  }
}
